<%= form_for :emp, url: emps_path do |f| %>
    <p>
      <%= f.label :first_name %><br>
      <%= f.text_field :first_name %>
    </p>

    <p>
      <%= f.label :last_name %><br>
      <%= f.text_field :last_name %>
    </p>

    <p>
      <%= f.label :role %><br>
      <%= f.text_field :role %>
    </p>

    <p>
      <%= f.submit %>
     </p>
<% end %>
<%= link_to 'Back', emps_path %>